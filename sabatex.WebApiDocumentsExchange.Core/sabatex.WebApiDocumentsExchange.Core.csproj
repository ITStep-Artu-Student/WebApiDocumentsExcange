<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net6.0;net7.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<NeutralLanguage>en-US</NeutralLanguage>
	<Authors>Serhiy Lakas</Authors>
	<Copyright>2023, Serhiy Lakas</Copyright>
	<Product>API Core controller for Exchange</Product>
	<Description>API Core for Exchange</Description>
	<Company>sabatex on git</Company>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageId>sabatex.WebApiDocumentsExchange.Core</PackageId>
	<PackageTags>#Exchange #API #Blazor</PackageTags>
	<Version>1.0.0</Version>

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Mvc.Core" Version="2.2.5" />
    <PackageReference Include="sabatex.Core" Version="1.0.0-alpha5" />
    <PackageReference Include="sabatex.WebApiDocumentsExchange.Models" Version="1.0.2" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
	<PackageReference Include="Microsoft.EntityFrameworkCore" Version="6.0.13" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net7.0'">
	  <PackageReference Include="Microsoft.EntityFrameworkCore" Version="7.0.0" />	
  </ItemGroup>

</Project>
